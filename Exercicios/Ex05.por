programa
{
	funcao inicio()
	{
		real Num1, Num2

		escreva("Digite o primeiro número: ")
		leia(Num1)

		escreva("Digite o segundo número: ")
		leia(Num2)

		se (Num1 > Num2) {
			escreva("O maior número é: ", Num1 , " Primeiro")
		}
		senao {
			escreva("O maior número é: ", Num2 , " Segundo")
		}
	}
}


